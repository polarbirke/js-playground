{
  "name": "js-playground",
  "version": "1.0.0",
  "description": "Playing with yarn, es2015 (babel), tape and rollup",
  "main": "main.js",
  "scripts": {
    "test": "nyc --reporter=html babel-tape-runner './test/**/*.js' | tap-spec",
    "lint": "gulp lint",
    "build": "gulp build"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [],
  "author": "polarbirke",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-tape-runner": "^3.0.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-file": "^0.4.0",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "dependencies": {}
