{
  "name": "js-playground",
  "version": "1.0.0",
  "description": "Playing with yarn, es2015 (babel), tape and rollup",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html babel-tape-runner './test/**/*.js' | tap-spec",
    "lint": "gulp lint",
    "build": "gulp build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [],
  "author": "polarbirke",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-file": "^0.3.0",
    "nyc": "^10.3.2",
    "pre-commit": "^1.2.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {}
}
